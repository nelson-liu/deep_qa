{
    "model_class": "AttentionSumReader",
    "model_serialization_prefix": "models/multiple_choice_qa/asreader_johannes",
    "preferred_backend": "theano",
    "encoder": {
        "default": {
            "type": "bi_gru",
            "output_dim": 384
        }
    },
    "seq2seq_encoder": {
        "default": {
            "type": "bi_gru",
            "encoder_params": {
                "output_dim": 384
            },
            "wrapper_params": {}
        }
    },
    "optimizer": {"type": "adam",
                  "clipnorm": 10.0,
                  "lr": 0.0005
                 },
    "embedding_dropout": 0.0,
    "patience": 0,
    "embedding_size": 256,
    "num_epochs": 1,
    "validation_files": ["/efs/data/dlfa/turk_johannes_questions/processed/dev_no_passage_with_background_as_passages.tsv"],
    "train_files": ["/efs/data/dlfa/turk_johannes_questions/processed/train_no_passage_with_background_as_passages.tsv"],
    "test_files": ["/efs/data/dlfa/turk_johannes_questions/processed/test_no_passage_with_background_as_passages.tsv"]
}
