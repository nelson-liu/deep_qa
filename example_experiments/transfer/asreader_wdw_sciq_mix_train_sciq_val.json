// train a model on a combined dataset with SciQ train and a subset of WhoDidWhat train and see how well it does on SciQ.
{
    "model_class": "AttentionSumReader",
    "model_serialization_prefix": "models/multiple_choice_qa/asreader_joint_wdw_sciq_train_sciq_test",
    "preferred_backend": "theano",
    "encoder": {
        "default": {
            "type": "bi_gru",
            "output_dim": 384
        }
    },
    "seq2seq_encoder": {
        "default": {
            "type": "bi_gru",
            "encoder_params": {
                "output_dim": 384
            },
            "wrapper_params": {}
        }
    },
    "optimizer": {"type": "adam",
                  "clipnorm": 10.0
                 },
    "embedding_dropout": 0.0,
    "patience": 0,
    "embedding_size": 256,
    "num_epochs": 5,
    "train_files": ["/efs/data/dlfa/sciq/sciq_and_who_did_what_combined/train/train.tsv"],
    "validation_files": ["/efs/data/dlfa/sciq/processed/test_no_passage_with_background_as_passages.tsv"]
}
